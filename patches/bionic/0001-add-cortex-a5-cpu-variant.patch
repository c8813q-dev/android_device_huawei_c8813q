From 15e799e51f86ae8df3f2c59b07a3de107b1b5fcf Mon Sep 17 00:00:00 2001
From: sakindia123 <sa6915@gmail.com>
Date: Sun, 28 Jul 2013 15:43:47 +0100
Subject: [PATCH] add cortex-a5 cpu variant

Change-Id: I3f72fa4524c09d3ea9abcc3a4a693f563e58ed53
---
 libc/arch-arm/arm.mk                 |    2 +-
 libc/arch-arm/cortex-a5/cortex-a5.mk |    1 +
 2 files changed, 2 insertions(+), 1 deletion(-)
 create mode 100644 include
 create mode 100644 libc/arch-arm/cortex-a5/cortex-a5.mk

diff --git a/include b/include
new file mode 100644
index 0000000..e69de29
diff --git a/libc/arch-arm/arm.mk b/libc/arch-arm/arm.mk
index e87ef38..d07d798 100644
--- a/libc/arch-arm/arm.mk
+++ b/libc/arch-arm/arm.mk
@@ -38,7 +38,7 @@ libc_common_src_files := \
     $(filter-out $(_LIBC_FORTIFY_FILES_TO_REMOVE),$(libc_common_src_files))
 
 ifeq ($(strip $(wildcard bionic/libc/arch-arm/$(TARGET_CPU_VARIANT)/$(TARGET_CPU_VARIANT).mk)),)
-$(error "TARGET_CPU_VARIANT not set or set to an unknown value. Possible values are cortex-a7, cortex-a8, cortex-a9, cortex-a15, krait. Use generic for devices that do not have a CPU similar to any of the supported cpu variants.")
+$(error "TARGET_CPU_VARIANT not set or set to an unknown value. Possible values are cortex-a5, cortex-a7, cortex-a8, cortex-a9, cortex-a15, krait. Use generic for devices that do not have a CPU similar to any of the supported cpu variants.")
 endif
 
 include bionic/libc/arch-arm/$(TARGET_CPU_VARIANT)/$(TARGET_CPU_VARIANT).mk
diff --git a/libc/arch-arm/cortex-a5/cortex-a5.mk b/libc/arch-arm/cortex-a5/cortex-a5.mk
new file mode 100644
index 0000000..944db2c
--- /dev/null
+++ b/libc/arch-arm/cortex-a5/cortex-a5.mk
@@ -0,0 +1 @@
+include bionic/libc/arch-arm/cortex-a9/cortex-a9.mk
-- 
1.7.9.5

